author = {Li-Bland, David and Meinrenken, Eckhard},
title = {{On the Van Est homomorphism for Lie groupoids}},
year = {2014},
eprint = {1403.1191v1},
eprinttype = {arxiv},
eprintclass = {math.DG},
month = mar,
note = {35 pages. To Appear.},
rating = {0},
journal = {L'Enseignement MathÃ©matique},
date-added = {2014-03-06T22:01:20GMT},
date-modified = {2014-03-06T22:02:15GMT},
abstract = {The Van Est homomorphism for a Lie groupoid G  M, as introduced by Weinstein-Xu, is a cochain map from the complex C^$\infty$(BG) of groupoid cochains to the Chevalley-Eilenberg complex C(A) of the Lie algebroid $\mathrm{A}$ of G. It was generalized by Weinstein, Mehta, and Abad-Crainic to a morphism from the Bott-Shulman-Stasheff complex $\Omega$(BG) to a (suitably defined) Weil algebra W(A). In this paper, we will give an approach to the Van Est map in terms of the Perturbation Lemma of homological algebra. This approach is used to establish the basic properties of the Van Est map. In particular, we show that on the normalized subcomplex, the Van Est map restricts to an algebra morphism.},
url = {http://arxiv.org/abs/1403.1191v1},
local-url = {file://localhost/Users/david/Dropbox/Library.papers3/Files/C3/C34A074F-5221-4352-ACAC-B66343E12445.pdf},
file = {{C34A074F-5221-4352-ACAC-B66343E12445.pdf:/Users/david/Dropbox/Library.papers3/Files/C3/C34A074F-5221-4352-ACAC-B66343E12445.pdf:application/pdf}},
uri = {\url{papers3://publication/uuid/27E9C4E4-5B80-4AE9-9DEF-CC26E9136272}}
}




